var app = Application.currentApplication()
app.includeStandardAdditions = true
app.activate()

ObjC.import("stdlib")
ObjC.import("stdio")

var res = app.{{.Operation}}({{json .Text}}, {{json .Options}})
if (res.gaveUp) {
	$.exit(5)
}
if (res.buttonReturned === {{json .Extra}}) {
	$.puts(res.buttonReturned)
	$.exit(1)
}